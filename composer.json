{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "fr3d/ldap-bundle": "^4.0",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^2.5",
        "friendsofsymfony/user-bundle": "2.1",
        "jms/serializer-bundle": "^3.3",
        "league/csv": "^9.2",
        "sensio/framework-extra-bundle": "^5.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.3.1",
        "symfony/browser-kit": "4.3.1",
        "symfony/cache": "4.3.1",
        "symfony/config": "4.3.1",
        "symfony/console": "4.3.1",
        "symfony/css-selector": "4.3.1",
        "symfony/debug": "4.3.1",
        "symfony/debug-pack": "1.0.6",
        "symfony/dependency-injection": "4.3.1",
        "symfony/dom-crawler": "4.3.1",
        "symfony/dotenv": "4.3.1",
        "symfony/event-dispatcher": "4.3.1",
        "symfony/expression-language": "4.3.1",
        "symfony/filesystem": "4.3.1",
        "symfony/flex": "^1.1",
        "symfony/form": "4.3.1",
        "symfony/framework-bundle": "4.3.1",
        "symfony/http-client": "4.3.1",
        "symfony/http-foundation": "4.3.1",
        "symfony/http-kernel": "4.3.1",
        "symfony/inflector": "4.3.1",
        "symfony/intl": "4.3.1",
        "symfony/ldap": "4.3.1",
        "symfony/monolog-bridge": "4.3.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/options-resolver": "4.3.1",
        "symfony/orm-pack": "1.0.*",
        "symfony/phpunit-bridge": "4.3.1",
        "symfony/process": "4.3.1",
        "symfony/property-access": "4.3.1",
        "symfony/property-info": "4.3.1",
        "symfony/security": "4.3.1",
        "symfony/security-bundle": "4.3.1",
        "symfony/serializer": "4.3.1",
        "symfony/serializer-pack": "1.0.2",
        "symfony/stopwatch": "4.3.1",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "4.3.1",
        "symfony/translation": "4.3.1",
        "symfony/twig-bridge": "4.3.1",
        "symfony/twig-bundle": "4.3.1",
        "symfony/validator": "4.3.1",
        "symfony/var-dumper": "4.3.1",
        "symfony/var-exporter": "4.3.1",
        "symfony/web-link": "4.3.1",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "4.3.1",
        "zendframework/zend-ldap": "^2.10"
    },
    "require-dev": {
        "symfony/debug-bundle": "4.3.1",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "*",
        "symfony/web-profiler-bundle": "4.3.1",
        "symfony/web-server-bundle": "4.3.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "AmorebietakoUdala\\SMSServiceBundle\\": "lib/AmorebietakoUdala/SMSServiceBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "4.3.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.1"
        }
    }
}
